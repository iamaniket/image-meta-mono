{
  "manifest_version": 3,
  "name": "Media Analyzer",
  "version": "0.1.0",
  "description": "Upload or capture an image and get a Gemini-powered description.",
  "action": {
    "default_popup": "index.html",
    "default_title": "Media Analyzer"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "http://localhost:3333/*",
    "https://*/*",
    "http://*/*"
  ]
}


